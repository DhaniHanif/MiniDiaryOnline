<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dhani.myapplication" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".PrintKey"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Mini Diary Mobile Online"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".FBLogin"
            android:label="Mini Diary Mobile Online" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SecondActivity"
            android:label="Mini Diary Mobile Online" >
        </activity>
        <activity
            android:name=".NewDiary"
            android:label="New Diary" >
        </activity>
        <activity
            android:name=".DiaryHistory"
            android:label="Diary History" >
        </activity>
        <activity
            android:name=".About"
            android:label="About" >
        </activity>
        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/app_id" />
        <activity
            android:name="com.facebook.FacebookActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
    </application>

</manifest>
